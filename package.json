{
    "name": "<NAME>",
    "version": "0.0.0",
    "description": "<DESCRIPTION>",
    "main": "src/index.js",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        },
        "./package.json": "./package.json"
    },
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build:ts": "tsc -b ./tsconfig.*.json",
        "build:web": "esbuild ./dist/esm/index.js --bundle --minify --sourcemap --target=es2015 --global-name=<NAME> --outfile=dist/web/index.js",
        "build": "pnpm clean; pnpm build:ts && pnpm build:web",
        "publish-package": "pnpm build && pnpm publish --access=public"
    },
    "dependencies": {},
    "devDependencies": {
        "esbuild": "^0.18.8",
        "typescript": "^5.1.3"
    },
    "author": "coderkearns",
    "license": "MIT"
}
